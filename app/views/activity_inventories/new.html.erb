<h1>New activity_inventory</h1>

<%= form_for(@activity_inventory) do |f| %>
  <%= error_messages(@activity_inventory) %>

  <p>
    <%= f.label :activity_id %><br />
    <%= f.text_field :activity_id %>
  </p>
  <p>
    <%= f.label :priority %><br />
    <%= f.text_field :priority %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', activity_inventories_path %>
